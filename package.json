{
  "name": "literal-life-church-messaging-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "deploy": "twilio-run deploy --username $TWILIO_API_KEY --password $TWILIO_API_SECRET",
    "deploy:prod": "twilio-run deploy --username $TWILIO_API_KEY --password $TWILIO_API_SECRET --environment=$",
    "start": "twilio serverless:start",
    "start:debug": "twilio serverless:start --inspect=\"\"",
    "start:tunnel": "twilio serverless:start --ngrok=\"\"",
    "start:tunnel:debug": "twilio serverless:start --inspect=\"\" --ngrok=\"\""
  },
  "dependencies": {
    "@twilio/runtime-handler": "1.2.2",
    "axios": "^0.27.2",
    "twilio": "3.76.1"
  },
  "devDependencies": {
    "twilio-run": "3.4.1"
  },
  "engines": {
    "node": "14"
  }
}
